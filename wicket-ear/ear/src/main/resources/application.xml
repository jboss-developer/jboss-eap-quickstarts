<application xmlns="http://java.sun.com/xml/ns/j2ee" version="1.4"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation= "http://java.sun.com/xml/ns/
					http://java.sun.com/xml/ns/application_1_4.xsd">

    <display-name>JBoss AS Quickstart: Wicket EAR</display-name>

    <module>
        <ejb>wicket-javaee-examples-1.0.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>wicket-javaee-examples-1.0.war</web-uri>
            <context-root>jboss-as-wicket-ear</context-root>
        </web>
    </module>

</application>
